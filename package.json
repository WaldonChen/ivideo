{
  "name": "ivideo",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Waldon Chen <waldonchen@gmail.com>",
  "private": true,
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node build/dev-runner.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test/unit test/e2e/specs",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node build/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node build/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node build/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node build/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node build/build.js",
    "build:web": "cross-env BUILD_TARGET=web node build/build.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
      "productName": "ivideo",
      "directories": {
          "output": "dist"
      },
      "files": [
          "dist/electron/**/*"
      ],
      "linux": {
          "icon": "build/logo.png"
      }
  },
  "dependencies": {
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "child-process": "^1.0.2",
    "chromedriver": "^2.27.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.0.2",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
